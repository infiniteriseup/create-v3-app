{
  "name": "create-v3-app",
  "version": "0.1.3",
  "description": "CLI to generate nuxt application using the V3 stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/loicpennequin/create-v3-app.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "create-v3-app",
    "vue",
    "nuxt",
    "tRPC",
    "typescript"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-v3-app": "./dist/index.js"
  },
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "start": "node dist/index.js",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern",
    "format:check": "yarn format --check",
    "release": "changeset version",
    "pub:beta": "yarn build && npm publish --tag beta",
    "pub:next": "yarn build && npm publish --tag next",
    "pub:release": "yarn build && npm publish"
  },
  "dependencies": {
    "@types/figlet": "^1.5.5",
    "asciify-image": "^0.1.9",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "execa": "^7.0.0",
    "figlet": "^1.5.2",
    "fs-extra": "^11.1.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.1.4",
    "ora": "6.1.2",
    "sort-package-json": "^2.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.14.0",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "type-fest": "^3.6.0",
    "typescript": "^4.9.5",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.10.1",
    "@sidebase/nuxt-auth": "^0.4.1",
    "@tanstack/vue-query": "^4.24.6",
    "@trpc/client": "^10.11.0",
    "@trpc/server": "^10.11.0",
    "@vueuse/core": "^9.12.0",
    "next-auth": "4.18.8",
    "nuxt-typed-router": "^2.3.5",
    "open-props": "^1.5.5",
    "superjson": "1.9.1",
    "trpc-nuxt": "^0.6.0",
    "vue-tsc": "^1.1.7",
    "zod": "^3.20.6",
    "@nuxt/image-edge": "^1.0.0-27919678.2f5b64b",
    "@nuxt/types": "^2.16.0",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/device": "^3.1.0",
    "@nuxtjs/i18n": "^8.0.0-beta.9",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@vueuse/nuxt": "^9.12.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "nuxt": "npm:nuxt3@latest",
    "nuxt-icon": "^0.2.11",
    "postcss-custom-media": "^9.1.2",
    "postcss-jit-props": "^1.0.11",
    "postcss-nesting": "^11.2.1",
    "prisma": "^4.10.1"
  }
}
